{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "vnetName": {
        "type": "string",
        "defaultValue": "sbvnet"
      },
      "subnetName": {
        "type": "string",
        "defaultValue": "sbsubnet"
      }
    },
    "resources": [
      {
        "name": "[parameters('vnetName')]",
        "type": "Microsoft.Network/virtualNetworks",
        "location": "eastus",
        "tags": {},
        "apiVersion": "2021-05-01",
        "properties": {
          "addressSpace": {
            "addressPrefixes": [
              "10.0.0.0/16"
            ]
          },
          "subnets": [
            {
              "name": "sbsubnet",
              "properties": {
                "addressPrefix": "10.0.0.0/24"
              }
            }
          ]
        }
      }
    ],
    "outputs": {
      "subnetId": {
        "value": "[resourceId('Microsoft.Network/virtualNetworks/subnet', parameters('vnetName'),parameters('subnetName'))]",
        "type": "string"
      }
    }
  }